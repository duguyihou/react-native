    "@testing-library/react-native": "12.4.3",
    "@types/react": "~18.2.14",
    "@types/react-test-renderer": "18.0.7",
    "@types/ws": "^8.5.10",
    "babel-loader": "^9.1.3",
    "babel-plugin-react-docgen-typescript": "^1.5.1",
    "jest": "^29.7.0",
    "jest-expo": "~50.0.4",
    "metro-react-native-babel-preset": "^0.77.0",
    "typescript": "^5.3.3"
