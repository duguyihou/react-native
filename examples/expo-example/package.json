{
  "name": "expo-example",
  "version": "8.0.0-alpha.3",
  "private": true,
  "main": "index.js",
  "scripts": {
    "android": "STORYBOOK_ENABLED=true expo start --android",
    "ios": "STORYBOOK_ENABLED=true expo start --ios",
    "web": "STORYBOOK_ENABLED=true expo start --web",
    "storybook": "STORYBOOK_ENABLED=true expo start",
    "storybook:web": "storybook dev -p 6006 -c ./.storybook-web",
    "build-web-storybook": "storybook build -c ./.storybook-web",
    "storybook-generate": "sb-rn-get-stories --config-path=./.storybook",
    "disabled-example": "expo start",
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "check": "tsc --noEmit",
    "test": "jest",
    "test:ci": "jest --runInBand"
  },
  "dependencies": {
    "@expo/metro-runtime": "~3.1.3",
    "@gorhom/bottom-sheet": "^4",
    "@react-native-async-storage/async-storage": "1.21.0",
    "@react-native-community/datetimepicker": "7.6.1",
    "@react-native-community/slider": "4.4.2",
    "@storybook/addon-essentials": "^8",
    "@storybook/addon-interactions": "^8",
    "@storybook/addon-links": "^8",
    "@storybook/addon-ondevice-actions": "^8.0.0-alpha.3",
    "@storybook/addon-ondevice-backgrounds": "^8.0.0-alpha.3",
    "@storybook/addon-ondevice-controls": "^8.0.0-alpha.3",
    "@storybook/addon-ondevice-notes": "^8.0.0-alpha.3",
    "@storybook/addon-react-native-server": "0.0.5",
    "@storybook/addon-react-native-web": "^0.0.22",
    "@storybook/blocks": "^8",
    "@storybook/builder-webpack5": "^8",
    "@storybook/core-common": "^8",
    "@storybook/docs-tools": "^8",
    "@storybook/global": "^5.0.0",
    "@storybook/react": "^8",
    "@storybook/react-native": "^8.0.0-alpha.3",
    "@storybook/react-native-theming": "^8.0.0-alpha.3",
    "@storybook/react-webpack5": "^8",
    "@storybook/test": "^8",
    "expo": "~50.0.14",
    "querystring": "^0.2.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.73.6",
    "react-native-gesture-handler": "~2.14.1",
    "react-native-reanimated": "~3.6.2",
    "react-native-safe-area-context": "4.8.2",
